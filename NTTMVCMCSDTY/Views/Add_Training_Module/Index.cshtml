@model NTTMVCMCSDTY.AddModuleInfo
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
   
</head>
<body>
    <h2>Add Training Module Details</h2>
    <form method="post">
        <div>
            <table style="width:80%" class="tbl table-striped">
                <tr>
                    <td>
                        ModuleName
                    </td>
                    <td>
                        @Html.TextBoxFor(m => m.Module_Name)

                    </td>
                </tr>
                <tr>
                    <td>
                        <input type="submit" value="AddModule" />
                    </td>
                </tr>
            </table>
            <br />
            <a href="~/Training_Module_Details/Index">Training module Page</a>
        </div>
        @ViewBag.Msg
    </form>
</body>
</html>
