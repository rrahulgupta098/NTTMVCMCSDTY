@model NTTMVCMCSDTY.ListModulevm
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
  
</head>
<body>
    <h2>Training Module Details</h2>
    <div>
        <table style="width:80%" class="tbl table-striped">
            <thead>
                <tr>
                    <td>
                        ModuleID
                    </td>
                    <td>
                        ModuleName
                    </td>
                    <td>
                        Delete Record
                    </td>
                    <td>
                        Edit
                    </td>

                </tr>
            </thead>
            <tbody>
                @foreach (var rec in Model.Lst)
                {
                    <tr>
                        <td>
                            @rec.ID
                        </td>
                        <td>
                            @rec.Module_Name
                        </td>
                        <td>
                            <a href="~/Training_Module_Details/DeleteModule?ID=@rec.ID">Delete</a>
                        </td>
                        <td>
                            <a href="~/Training_Module_Details/EditModule?ID=@rec.ID">Edit</a>
                        </td>

                    </tr>
                }
            </tbody>
        </table>
        <a href="~/Add_Training_Module/Index">Add Module</a>
        <br />
        <a href="~/ADMIN_DASHBOARD/Index">Go to Home</a>

    </div>
</body>
</html>
